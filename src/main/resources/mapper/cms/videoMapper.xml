<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.liyang.mapper.cms.videoMapper">
    <resultMap id="BaseResultMap" type="com.liyang.entity.cms.video" >
        <result column="name" jdbcType="VARCHAR" property="name" />
        <result column="sub" jdbcType="VARCHAR" property="sub" />
        <result column="type" jdbcType="VARCHAR" property="type" />
        <result column="pic" jdbcType="VARCHAR" property="pic" />
        <result column="actor" jdbcType="VARCHAR" property="actor" />
        <result column="remarks" jdbcType="VARCHAR" property="remarks" />
        <result column="area" jdbcType="VARCHAR" property="area" />
        <result column="lang" jdbcType="VARCHAR" property="lang" />
        <result column="year" jdbcType="VARCHAR" property="year" />
        <result column="content" jdbcType="VARCHAR" property="content" />
        <result column="url" jdbcType="VARCHAR" property="playUrl" />
    </resultMap>
    <select id="queryVideoByNameAndPage" resultMap="BaseResultMap">
        call maccms.query_vod_detailed(#{name}, #{page}, #{limit});
    </select>
</mapper>
