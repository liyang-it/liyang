<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.liyang.mapper.music.controlMapper">
    <resultMap id="BaseResultMap" type="com.liyang.entity.music.control" >
        <result column="isToGd" jdbcType="INTEGER" property="isToGd" />
    </resultMap>
    <select id="queryIsToGd" resultType="java.lang.Integer" >
        select isToGd from music_control
    </select>
    <update id="setIsToGd" parameterType="java.lang.Integer">
        update music_control set isToGd = #{param}
    </update>

</mapper>
