<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.liyang.mapper.PlaylisttimeMapper">
    <resultMap id="BaseResultMap" type="com.liyang.entity.Playlisttime" >
        <result column="id" jdbcType="INTEGER" property="id" />
        <result column="playlistcreatedate" jdbcType="DATE" property="playlistcreatedate" />
    </resultMap>
    <insert id="addPlayListTime" parameterType="java.lang.String" >
        insert into playListTime
            (id,playlistcreatedate)
            values
             (#{id}, current_date())
    </insert>
    <delete id="deletePlayListTime">
        delete from playListTime
    </delete>
</mapper>
